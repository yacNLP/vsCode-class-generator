{
  "name": "Form-generator-topaz",
  "displayName": "Form generator Topaz",
  "description": "Description of your extension",
  "version": "0.0.1",
  "publisher": "Yacine HADDAD",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:extension.generateForm"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateForm",
        "title": "Generate Form"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.generateForm"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "@types/node": "^20.8.9",
    "fs": "0.0.1-security",
    "ts-morph": "^20.0.0",
    "typescript": "^5.2.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^16.0.0",
    "@types/vscode": "^1.58.0",
    "typescript": "^4.3.5"
  }
}
